{
  "name": "google-timeline-converter",
  "version": "1.0.0",
  "description": "A client-side tool to convert Google Location History JSON to CSV",
  "main": "dist/bundle.js",
  "scripts": {
    "build": "webpack --mode production && cp converter.html dist/index.html && cp geo-enrichment.js dist/",
    "dev": "webpack --mode development --watch",
    "serve": "http-server -o"
  },
  "keywords": [
    "google",
    "timeline",
    "location",
    "history",
    "csv",
    "converter"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "copy-webpack-plugin": "^11.0.0",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "http-server": "^14.1.1"
  },
  "dependencies": {
    "papaparse": "^5.4.1",
    "oboe": "^2.1.5",
    "@types/papaparse": "^5.3.7",
    "@types/oboe": "^2.0.1"
  }
} 